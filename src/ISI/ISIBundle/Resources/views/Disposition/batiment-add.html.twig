{% extends 'ISIBundle:Disposition:index.html.twig'%}



{% block title 'Ajout d\'un nouveau bâtiment' %}



{% block body %}
  <div class="ui right aligned segment">
    <h1 class="ui header">
      <i class="student icon"></i>{{ 'Disposition en salle de classe'|trans }}
    </h1>
  </div>
  <h2>Ajout d'un nouveau bâtiment</h2>
  <div>
    <form action="" method="post" class="ui form">
      <div class="two fields">
        <div class="field">
          <label for="nom">Nom du bâtiment</label>
          <input type="text" name="nom" id="nom">
        </div>
      </div>
      <button type="submit" class="ui positive button">Valider</button>
    </form>
  </div>
{% endblock %}


{% block javascripts %}
{% endblock %}
