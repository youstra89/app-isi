{{ form_start(form, { 'attr': {'class': 'ui form'} }) }}
    <div class="three fields">
      <div class="field">
        <label for="">Date </label>
          <input type="date" name="date" value="">
      </div>
      <div class="field">
          {{ form_row(form.imam, { 'attr': {'class': 'uk-input'} }) }}
      </div>
      <div class="field">
          {{ form_row(form.mosquee, { 'attr': {'class': 'uk-input'} }) }}
      </div>
    </div>
    {{ form_rest(form) }}
    <button class="ui primary button">{{ button|default('Enregistrer') }}</button>
  </div><br>
{{ form_end(form) }}
